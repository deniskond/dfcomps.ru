{
    "name": "dfcomps-websocket-server",
    "version": "1.0.0",
    "description": "Websocket server for dfcomps",
    "main": "index.js",
    "scripts": {
        "jest": "jest",
        "tsc": "tsc",
        "start": "rimraf dist && npm run tsc && copyfiles config.json dist && cross-env ENV=prod node dist/server.js",
        "start:local": "rimraf dist && npm run tsc && copyfiles config.json dist && cross-env ENV=test node dist/server.js",
        "test": "concurrently \"npm run start:local\" \"jest --runInBand\"",
        "stress-test": "rimraf dist && npm run tsc && copyfiles config.json dist && concurrently \"cross-env ENV=test node dist/server.js\" \"node dist/1v1/tests/client-stress-testing.js\""
    },
    "keywords": [
        "dfcomps",
        "defrag"
    ],
    "author": "Nosf",
    "license": "MIT",
    "dependencies": {
        "@types/express": "^4.17.6",
        "@types/faker": "^5.1.7",
        "@types/jest": "^26.0.21",
        "@types/lodash": "^4.14.168",
        "@types/node": "^14.14.35",
        "@types/uuid": "^8.0.0",
        "@types/ws": "^7.2.5",
        "axios": "^0.21.1",
        "concurrently": "^6.0.0",
        "copyfiles": "^2.4.0",
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
        "faker": "^5.4.0",
        "jest": "^26.6.3",
        "lodash": "^4.17.21",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.5",
        "ts-jest": "^26.5.4",
        "typescript": "^3.9.3",
        "uuid": "^8.1.0",
        "ws": "^7.3.0"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "dist"
        ]
    }
}
