    "@nestjs/platform-express": "10.0.2",
    "@nestjs/typeorm": "10.0.0",
    "chart.js": "4.3.3",
    "discord.js": "12.3.1",
    "express": "4.17.3",
    "flag-icon-css": "3.5.0",
