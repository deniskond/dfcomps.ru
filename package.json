{
  "name": "dfcomps.ru",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && husky install",
    "frontend": "nx serve frontend --open",
    "frontend-local": "nx serve frontend --open --configuration=local-backend",
    "frontend:build": "nx build frontend",
    "frontend:server": "superstatic dist/apps/frontend",
    "backend": "nx serve backend",
    "backend:build": "nx build backend",
    "backend:setup": "docker-compose -f apps/backend/database/docker-compose.yml up -d --wait && npm run backend:restore-db",
    "backend:restore-db": "docker cp apps/backend/database/database.tar local_pgdb:/var/lib/postgresql/database.tar && docker exec local_pgdb createdb -h localhost -p 5432 -U user dfcomps && docker exec local_pgdb pg_restore -U user -d dfcomps /var/lib/postgresql/database.tar",
    "backend:save-db": "docker exec local_pgdb pg_dump -U user dfcomps -f /var/lib/postgresql/database.tar -F t && docker cp local_pgdb:/var/lib/postgresql/database.tar apps/backend/database/database.tar",
    "backend:liquibase-changelog": "docker-compose -f apps/backend/database/docker-compose.liquibase.yml up -d --wait && node -e \"setTimeout(()=>{},3000)\" && docker cp liquibase_container:/liquibase/dfcomps.changelog.json apps/backend/database/dfcomps.changelog.json",
    "discord-bot": "nx serve discord-bot",
    "discord-bot:build": "nx build discord-bot",
    "1v1-server:prod": "cross-env ENV=prod nx serve 1v1-server",
    "1v1-server:local": "cross-env ENV=test nx serve 1v1-server",
    "1v1-server:test": "concurrently \"npm run 1v1-server:local\" \"wait-on tcp:3000 && nx test 1v1-server --runInBand\"",
    "1v1-server:test:ci": "concurrently \"cross-env LOGS=none npm run 1v1-server:local\" \"wait-on tcp:3000 && nx test 1v1-server --runInBand\"",
    "1v1-server:stress-test": "concurrently \"cross-env E2E=true npm run 1v1-server:local\" \"wait-on tcp:3000 && ts-node --project apps/1v1-server/tsconfig.app.json apps/1v1-server/src/app/1v1/tests/client-stress-testing.ts\"",
    "1v1-server:stress-test:ci": "concurrently \"cross-env LOGS=none E2E=true npm run 1v1-server:local\" \"wait-on tcp:3000 && ts-node --project apps/1v1-server/tsconfig.app.json apps/1v1-server/src/app/1v1/tests/client-stress-testing.ts\"",
    "1v1-server:build": "nx build 1v1-server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.0",
    "@angular/cdk": "16.2.0",
    "@angular/common": "16.2.0",
    "@angular/compiler": "16.2.0",
    "@angular/core": "16.2.0",
    "@angular/forms": "16.2.0",
